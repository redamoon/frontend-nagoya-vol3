    text="テキスト01テキスト01テキスト01テキスト01テキスト01テキスト01テキスト01テキスト01テキスト01テキスト01テキスト01テキスト01テキスト01テキスト01"
   />
  <mt:Var name="_HeadingTemplate" key="HeadingTemplate" text="見出し02テキスト見出し02テキスト" />
  <mt:Var
    name="_TextTemplate"
    key="TextTemplate"
