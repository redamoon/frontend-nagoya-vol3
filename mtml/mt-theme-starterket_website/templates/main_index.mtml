<mt:ignore>
========================================
Template Name : index.html
Template Type : main index template
Template Note : トップページ出力用
========================================
</mt:ignore>
<mt:Include module="config" parent="1" />
<mt:ignore>** ローカル変数 **</mt:ignore>
<mt:SetVars>
base_title=トップページ
page_class=index
site_label_data_information_name=お知らせのボタン
site_label_data_information_link=/information/
site_label_data_company_name=会社概要のボタン
site_label_data_company_link=/company/
</mt:SetVars>
<mt:Ignore>新着情報記事セット</mt:Ignore>
<mt:MultiBlog blog_id="2">
  <mt:Entries lastn="5">
    <mt:SetHashVar name="entry">
      <mt:EntryDate format="$dateformat" setvar="date" />
      <mt:EntryTitle setvar="title" />
      <mt:EntryPermalink setvar="link" />
    </mt:SetHashVar>
    <mt:Var name="push(entries)" value="$entry" />
  </mt:Entries>
</mt:MultiBlog>

<mt:ignore>** 表示部分 **</mt:ignore>
<mt:Unless name="compress" regex_replace="/^\s*\n/gm","">
<mt:Include module="component" parent="1" />
<mt:Include module="html_head" parent="1" />
<mt:Include module="header" parent="1" />
<mt:Include module="mainvisual" parent="1" />
<div class="uk-container uk-container-center uk-margin-top uk-margin-large-bottom">
  <div class="uk-text-center uk-margin-medium" uk-grid>
    <div class="uk-width-medium-1-3 uk-row-first" uk-grid>
