  <div class="uk-text-center uk-margin-medium" uk-grid>
    <div class="uk-width-medium-1-3 uk-row-first" uk-grid>
      <div class="uk-width-1-3 uk-card">
        <mt:Var
          name="_BtnTemplate"
          key="BtnTemplate"
          class="uk-button-danger uk-width-1-1"
